 R_TYPE = 6'b000000,
		 ADDI   = 6'b001000,
		BEQ   = 6'b000100,  
		J   = 6'b000010;
END = 6'b111111;

001000_00001_00000_0000000000000100 // x1 = 4
001000_00010_00000_0000000000000100 // y1 = 4
001000_00011_00000_0000000000000010 // x2 = 2 
001000_00100_00000_0000000000000010 // y2 = 2

000000_00101_00001_00011_00000_100010 // x1 - x2
000000_00110_00010_00100_00000_100010 // y1 - y2

000000_00101_00101_00010_00000_011000 // diff x squared
000000_00110_00110_00110_00000_011000 // diff y squared

000000_00111_00110_00101_00000_100000 // sum the differences

111111_00111_00000_00000_00000_000000 // print
